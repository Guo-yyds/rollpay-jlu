{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { adminAdd } from '@/api/admin';\nexport default {\n  data() {\n    return {\n      dialogVisible: false,\n      // 对话框显示状态\n      // 表单数据\n      username: 'test5',\n      password: '123456',\n      name: '张武',\n      email: '333@qq.com',\n      SSN: '111',\n      tax: '10',\n      detection: '401',\n      telephone: '1234567890',\n      kind: '0',\n      hoursal: '500',\n      sal: '',\n      com: '',\n      hourlimit: '10',\n      way: '0',\n      address: '吉林长春',\n      bankname: '工商银行',\n      banknumber: '1234567890',\n      // 分页器数据\n      dataList: [],\n      // 数据列表\n      total: 0,\n      // 总记录数\n      currentPage: 1,\n      // 当前页码\n      pageSize: 10 // 每页显示的记录数\n    };\n  },\n\n  methods: {\n    logout() {\n      this.$store.dispatch('logout');\n      this.$router.push('/adminLogin');\n    },\n    // 以下是对话框相关的方法\n\n    // 点击添加按钮，显示对话框\n    showDialog() {\n      this.dialogVisible = true;\n    },\n    // 关闭对话框\n    closeDialog() {\n      this.dialogVisible = false;\n      this.resetForm();\n    },\n    // 重置表单数据\n    resetForm() {\n      this.$refs.form.resetFields();\n      this.formData = {\n        name: '',\n        age: null\n      };\n    },\n    // 保存数据\n    async saveData() {\n      // 在这里可以进行数据保存操作\n      const res = await adminAdd(this.username, this.password, this.name, this.email, this.SSN, this.tax, this.detection, this.telephone, this.kind, this.hoursal, this.sal, this.com, this.hourlimit, this.way, this.address, this.bankname, this.banknumber);\n      console.log(res);\n      if (res.code !== 1) {\n        this.$message(res.msg);\n        return;\n      }\n      this.$message('添加成功');\n      this.closeDialog();\n    },\n    // 以下是分页器相关的方法\n    // 处理页码变化事件\n    handleCurrentChange(page) {\n      this.currentPage = page;\n    },\n    // 处理每页显示条数变化事件\n    handleSizeChange(size) {\n      this.pageSize = size;\n    },\n    // 获取数据列表\n    getDataList() {\n      // 根据当前页码和每页记录数发送请求获取数据列表\n      // 示例中使用 setTimeout 模拟异步请求\n      setTimeout(() => {\n        // 假设从后端获取的数据为 response\n        const response = {\n          total: 100,\n          // 总记录数\n          data: [{\n            id: 1,\n            name: '数据1'\n          }, {\n            id: 2,\n            name: '数据2'\n          }\n          // ...\n          ]\n        };\n\n        // 更新数据列表和总记录数\n        this.dataList = response.data;\n        this.total = response.total;\n      }, 500);\n    }\n  },\n  mounted() {\n    this.getDataList(); // 初始化时获取数据列表\n  }\n};","map":{"version":3,"names":["adminAdd","data","dialogVisible","username","password","name","email","SSN","tax","detection","telephone","kind","hoursal","sal","com","hourlimit","way","address","bankname","banknumber","dataList","total","currentPage","pageSize","methods","logout","$store","dispatch","$router","push","showDialog","closeDialog","resetForm","$refs","form","resetFields","formData","age","saveData","res","console","log","code","$message","msg","handleCurrentChange","page","handleSizeChange","size","getDataList","setTimeout","response","id","mounted"],"sources":["src/views/adminHome.vue"],"sourcesContent":["<template>\r\n  <el-container style=\"height: 500px; border: 1px solid #eee\">\r\n    <el-aside width=\"200px\" style=\"background-color: rgb(238, 241, 246)\">\r\n      <el-menu :default-openeds=\"['1', '3']\">\r\n        <el-menu-item><i class=\"el-icon-s-custom\"></i>管理员工</el-menu-item>\r\n        <el-menu-item><i class=\"el-icon-document\"></i>生成管理员报告</el-menu-item>\r\n      </el-menu>\r\n    </el-aside>\r\n\r\n    <el-container>\r\n      <el-header style=\"text-align: right; font-size: 16px\">\r\n        用户名:\r\n        <span>{{this.$store.state.userInfo.username}}</span>\r\n        <!-- 空格 -->\r\n        &nbsp;&nbsp;\r\n        <el-button @click=\"logout\">退出登录</el-button>\r\n      </el-header>\r\n\r\n      <el-main>\r\n        <el-button  @click=\"showDialog\">添加员工</el-button>\r\n        <el-button type=\"primary\">修改员工</el-button>\r\n        <el-button type=\"success\">删除员工</el-button>\r\n        <el-dialog\r\n          title=\"添加信息\"\r\n          :visible=\"dialogVisible\"\r\n          @close=\"closeDialog\"\r\n        >\r\n          <!-- 对话框内容 -->\r\n          <el-form ref=\"form\" :model=\"formData\">\r\n            <el-form-item label=\"用户名\"><el-input v-model=\"username\"></el-input></el-form-item>\r\n            <el-form-item label=\"密码\"><el-input v-model=\"password\"></el-input></el-form-item>\r\n            <el-form-item label=\"姓名\"><el-input v-model=\"name\"></el-input></el-form-item>\r\n            <el-form-item label=\"邮箱\"><el-input v-model=\"email\"></el-input></el-form-item>\r\n            <el-form-item label=\"社保号\"><el-input v-model=\"SSN\"></el-input></el-form-item>\r\n            <el-form-item label=\"税号\"><el-input v-model=\"tax\"></el-input></el-form-item>\r\n            <el-form-item label=\"检测号\"><el-input v-model=\"detection\"></el-input></el-form-item>\r\n            <el-form-item label=\"电话号码\"><el-input v-model=\"telephone\"></el-input></el-form-item>\r\n            <el-form-item label=\"员工类型\">\r\n              <el-radio-group v-model=\"kind\">\r\n                <el-radio label=\"0\">Hourly</el-radio>\r\n                <el-radio label=\"1\">Salaried</el-radio>\r\n                <el-radio label=\"2\">Commission</el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n            <el-form-item label=\"时薪\" v-show=\"kind == 0\"><el-input v-model=\"hoursal\"></el-input></el-form-item>\r\n            <el-form-item label=\"月薪\" v-show=\"kind != 0\"><el-input v-model=\"sal\"></el-input></el-form-item>\r\n            <el-form-item label=\"佣金\" v-show=\"kind == 2\"><el-input v-model=\"com\"></el-input></el-form-item>\r\n            <el-form-item label=\"小时限制\"><el-input v-model=\"hourlimit\"></el-input></el-form-item>\r\n            <el-form-item label=\"支付方式\">\r\n              <el-radio-group v-model=\"way\">\r\n                <el-radio label=\"0\">Mail</el-radio>\r\n                <el-radio label=\"1\">Pickup</el-radio>\r\n                <el-radio label=\"2\">Deposit</el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n            <el-form-item label=\"地址\" v-show=\"way == 1\"><el-input v-model=\"address\"></el-input></el-form-item>\r\n            <el-form-item label=\"银行名称\" v-show=\"way == 2\"><el-input v-model=\"bankname\"></el-input></el-form-item>\r\n            <el-form-item label=\"银行账号\" v-show=\"way == 2\"><el-input v-model=\"banknumber\"></el-input></el-form-item>\r\n          </el-form>\r\n          <!-- 按钮 -->\r\n          <div slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"closeDialog\">取消</el-button>\r\n            <el-button type=\"primary\" @click=\"saveData\">保存</el-button>\r\n          </div>\r\n        </el-dialog>\r\n\r\n        <br>\r\n        <br>\r\n\r\n        <div>\r\n          <!-- 数据列表 -->\r\n          <ul>\r\n            <li v-for=\"item in dataList\" :key=\"item.id\">{{ item.name }}</li>\r\n          </ul>\r\n\r\n          <!-- 分页组件 -->\r\n          <el-pagination\r\n            :total=\"total\"\r\n            :current-page=\"currentPage\"\r\n            :page-size=\"pageSize\"\r\n            @current-change=\"handleCurrentChange\"\r\n          ></el-pagination>\r\n        </div>\r\n\r\n      </el-main>\r\n    </el-container>\r\n  </el-container>\r\n</template>\r\n<style>\r\n  .el-header {\r\n    background-color: #B3C0D1;\r\n    color: #333;\r\n    line-height: 60px;\r\n  }\r\n\r\n  .el-aside {\r\n    color: #333;\r\n  }\r\n</style>\r\n\r\n<script>\r\nimport { adminAdd } from '@/api/admin'\r\nexport default {\r\n  data () {\r\n    return {\r\n      dialogVisible: false, // 对话框显示状态\r\n      // 表单数据\r\n      username: 'test5',\r\n      password: '123456',\r\n      name: '张武',\r\n      email: '333@qq.com',\r\n      SSN: '111',\r\n      tax: '10',\r\n      detection: '401',\r\n      telephone: '1234567890',\r\n      kind: '0',\r\n      hoursal: '500',\r\n      sal: '',\r\n      com: '',\r\n      hourlimit: '10',\r\n      way: '0',\r\n      address: '吉林长春',\r\n      bankname: '工商银行',\r\n      banknumber: '1234567890',\r\n      // 分页器数据\r\n      dataList: [], // 数据列表\r\n      total: 0, // 总记录数\r\n      currentPage: 1, // 当前页码\r\n      pageSize: 10 // 每页显示的记录数\r\n    }\r\n  },\r\n  methods: {\r\n    logout () {\r\n      this.$store.dispatch('logout')\r\n      this.$router.push('/adminLogin')\r\n    },\r\n    // 以下是对话框相关的方法\r\n\r\n    // 点击添加按钮，显示对话框\r\n    showDialog () {\r\n      this.dialogVisible = true\r\n    },\r\n    // 关闭对话框\r\n    closeDialog () {\r\n      this.dialogVisible = false\r\n      this.resetForm()\r\n    },\r\n    // 重置表单数据\r\n    resetForm () {\r\n      this.$refs.form.resetFields()\r\n      this.formData = {\r\n        name: '',\r\n        age: null\r\n      }\r\n    },\r\n    // 保存数据\r\n    async saveData () {\r\n      // 在这里可以进行数据保存操作\r\n      const res = await adminAdd(this.username, this.password, this.name, this.email, this.SSN, this.tax, this.detection, this.telephone, this.kind, this.hoursal, this.sal, this.com, this.hourlimit, this.way, this.address, this.bankname, this.banknumber)\r\n      console.log(res)\r\n      if (res.code !== 1) {\r\n        this.$message(res.msg)\r\n        return\r\n      }\r\n      this.$message('添加成功')\r\n      this.closeDialog()\r\n    },\r\n    // 以下是分页器相关的方法\r\n    // 处理页码变化事件\r\n    handleCurrentChange (page) {\r\n      this.currentPage = page\r\n    },\r\n    // 处理每页显示条数变化事件\r\n    handleSizeChange (size) {\r\n      this.pageSize = size\r\n    },\r\n    // 获取数据列表\r\n    getDataList () {\r\n      // 根据当前页码和每页记录数发送请求获取数据列表\r\n      // 示例中使用 setTimeout 模拟异步请求\r\n      setTimeout(() => {\r\n        // 假设从后端获取的数据为 response\r\n        const response = {\r\n          total: 100, // 总记录数\r\n          data: [\r\n            { id: 1, name: '数据1' },\r\n            { id: 2, name: '数据2' }\r\n            // ...\r\n          ]\r\n        }\r\n\r\n        // 更新数据列表和总记录数\r\n        this.dataList = response.data\r\n        this.total = response.total\r\n      }, 500)\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getDataList() // 初始化时获取数据列表\r\n  }\r\n\r\n}\r\n</script>\r\n"],"mappings":";AAqGA,SAAAA,QAAA;AACA;EACAC,KAAA;IACA;MACAC,aAAA;MAAA;MACA;MACAC,QAAA;MACAC,QAAA;MACAC,IAAA;MACAC,KAAA;MACAC,GAAA;MACAC,GAAA;MACAC,SAAA;MACAC,SAAA;MACAC,IAAA;MACAC,OAAA;MACAC,GAAA;MACAC,GAAA;MACAC,SAAA;MACAC,GAAA;MACAC,OAAA;MACAC,QAAA;MACAC,UAAA;MACA;MACAC,QAAA;MAAA;MACAC,KAAA;MAAA;MACAC,WAAA;MAAA;MACAC,QAAA;IACA;EACA;;EACAC,OAAA;IACAC,OAAA;MACA,KAAAC,MAAA,CAAAC,QAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACA;;IAEA;IACAC,WAAA;MACA,KAAA5B,aAAA;IACA;IACA;IACA6B,YAAA;MACA,KAAA7B,aAAA;MACA,KAAA8B,SAAA;IACA;IACA;IACAA,UAAA;MACA,KAAAC,KAAA,CAAAC,IAAA,CAAAC,WAAA;MACA,KAAAC,QAAA;QACA/B,IAAA;QACAgC,GAAA;MACA;IACA;IACA;IACA,MAAAC,SAAA;MACA;MACA,MAAAC,GAAA,SAAAvC,QAAA,MAAAG,QAAA,OAAAC,QAAA,OAAAC,IAAA,OAAAC,KAAA,OAAAC,GAAA,OAAAC,GAAA,OAAAC,SAAA,OAAAC,SAAA,OAAAC,IAAA,OAAAC,OAAA,OAAAC,GAAA,OAAAC,GAAA,OAAAC,SAAA,OAAAC,GAAA,OAAAC,OAAA,OAAAC,QAAA,OAAAC,UAAA;MACAqB,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA,IAAAA,GAAA,CAAAG,IAAA;QACA,KAAAC,QAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MACA;MACA,KAAAD,QAAA;MACA,KAAAZ,WAAA;IACA;IACA;IACA;IACAc,oBAAAC,IAAA;MACA,KAAAxB,WAAA,GAAAwB,IAAA;IACA;IACA;IACAC,iBAAAC,IAAA;MACA,KAAAzB,QAAA,GAAAyB,IAAA;IACA;IACA;IACAC,YAAA;MACA;MACA;MACAC,UAAA;QACA;QACA,MAAAC,QAAA;UACA9B,KAAA;UAAA;UACApB,IAAA,GACA;YAAAmD,EAAA;YAAA/C,IAAA;UAAA,GACA;YAAA+C,EAAA;YAAA/C,IAAA;UAAA;UACA;UAAA;QAEA;;QAEA;QACA,KAAAe,QAAA,GAAA+B,QAAA,CAAAlD,IAAA;QACA,KAAAoB,KAAA,GAAA8B,QAAA,CAAA9B,KAAA;MACA;IACA;EACA;EACAgC,QAAA;IACA,KAAAJ,WAAA;EACA;AAEA"},"metadata":{},"sourceType":"module","externalDependencies":[]}