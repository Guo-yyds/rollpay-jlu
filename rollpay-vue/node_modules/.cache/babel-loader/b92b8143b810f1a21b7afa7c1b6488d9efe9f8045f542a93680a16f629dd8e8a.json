{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { adminAdd, adminList } from '@/api/admin';\nexport default {\n  data() {\n    return {\n      activeMenu: 'show',\n      dialogVisible: false,\n      // 对话框显示状态\n      // 表单数据\n      username: '',\n      password: '',\n      name: '',\n      email: '',\n      SSN: '',\n      tax: '',\n      detection: '',\n      telephone: '',\n      kind: '',\n      hoursal: '',\n      sal: '',\n      com: '',\n      hourlimit: '',\n      way: '',\n      address: '',\n      bankname: '',\n      banknumber: ''\n    };\n  },\n  methods: {\n    handleMenuSelect(index) {\n      this.activeMenu = index;\n      this.$router.push({\n        path: '/' + index\n      });\n    },\n    logout() {\n      this.$store.dispatch('logout');\n      this.$router.push('/adminLogin');\n    },\n    // 以下是对话框相关的方法\n\n    // 点击添加按钮，显示对话框\n    showDialog() {\n      this.dialogVisible = true;\n    },\n    // 关闭对话框\n    closeDialog() {\n      this.dialogVisible = false;\n      this.resetForm();\n    },\n    // 重置表单数据\n    resetForm() {\n      this.$refs.form.resetFields();\n\n      // 重置表单数据\n      this.username = '';\n      this.password = '';\n      this.name = '';\n      this.email = '';\n      this.SSN = '';\n      this.tax = '';\n      this.detection = '';\n      this.telephone = '';\n      this.kind = '';\n      this.hoursal = '';\n      this.sal = '';\n      this.com = '';\n      this.hourlimit = '';\n      this.way = '';\n      this.address = '';\n      this.bankname = '';\n      this.banknumber = '';\n    },\n    // 保存数据\n    async saveData() {\n      // 在这里可以进行数据保存操作\n      const res = await adminAdd(this.username, this.password, this.name, this.email, this.SSN, this.tax, this.detection, this.telephone, this.kind, this.hoursal, this.sal, this.com, this.hourlimit, this.way, this.address, this.bankname, this.banknumber);\n      console.log(res);\n      if (res.code !== 1) {\n        this.$message(res.msg);\n        return;\n      }\n      this.$message('添加成功');\n      this.closeDialog();\n    },\n    // 以下是分页器相关的方法\n    async fetchData() {\n      // 模拟异步获取数据\n      // 这里假设从后端获取了100条数据\n      const res = await adminList(this.currentPage, this.pageSize);\n      console.log(res.data.records);\n      this.items = res.data.records;\n      this.totalItems = res.data.total;\n    },\n    handleCurrentChange(newPage) {\n      // 处理页码变化\n      this.currentPage = newPage;\n      this.fetchData();\n    },\n    handleSizeChange(newSize) {\n      // 处理页面大小变化\n      this.pageSize = newSize;\n      this.currentPage = 1;\n      this.fetchData();\n    }\n  }\n};","map":{"version":3,"names":["adminAdd","adminList","data","activeMenu","dialogVisible","username","password","name","email","SSN","tax","detection","telephone","kind","hoursal","sal","com","hourlimit","way","address","bankname","banknumber","methods","handleMenuSelect","index","$router","push","path","logout","$store","dispatch","showDialog","closeDialog","resetForm","$refs","form","resetFields","saveData","res","console","log","code","$message","msg","fetchData","currentPage","pageSize","records","items","totalItems","total","handleCurrentChange","newPage","handleSizeChange","newSize"],"sources":["src/views/adminHome.vue"],"sourcesContent":["<template>\r\n  <el-container style=\"height: 100%; border: 1px solid #eee\">\r\n    <el-aside width=\"200px\" style=\"background-color: rgb(238, 241, 246)\">\r\n      <el-menu :default-openeds=\"['1', '3']\">\r\n        <el-submenu index=\"1\">\r\n          <template slot=\"title\"><i class=\"el-icon-s-custom\"></i>管理员工</template>\r\n          <el-menu :default-active=\"activeMenu\" @select=\"handleMenuSelect\">\r\n            <el-menu-item index=\"show\">显示员工</el-menu-item>\r\n            <el-menu-item index=\"insert\">添加员工</el-menu-item>\r\n            <el-menu-item index=\"update\">修改员工</el-menu-item>\r\n            <el-menu-item index=\"delete\">删除员工</el-menu-item>\r\n          </el-menu>\r\n        </el-submenu>\r\n\r\n        <el-submenu index=\"2\">\r\n          <template slot=\"title\"><i class=\"el-icon-document\"></i>生成管理员报告</template>\r\n        </el-submenu>\r\n      </el-menu>\r\n    </el-aside>\r\n\r\n    <el-container>\r\n      <el-header style=\"text-align: right; font-size: 16px\">\r\n        用户名:\r\n        <span>{{this.$store.state.userInfo.username}}</span>\r\n        <!-- 空格 -->\r\n        &nbsp;&nbsp;\r\n        <el-button @click=\"logout\">退出登录</el-button>\r\n      </el-header>\r\n\r\n      <el-main height=\"100%\">\r\n        <router-view></router-view>\r\n\r\n        <el-dialog\r\n          title=\"添加信息\"\r\n          :visible=\"dialogVisible\"\r\n          @close=\"closeDialog\"\r\n        >\r\n          <!-- 对话框内容 -->\r\n          <el-form ref=\"form\">\r\n            <el-form-item label=\"用户名\"><el-input v-model=\"username\"></el-input></el-form-item>\r\n            <el-form-item label=\"密码\"><el-input v-model=\"password\"></el-input></el-form-item>\r\n            <el-form-item label=\"姓名\"><el-input v-model=\"name\"></el-input></el-form-item>\r\n            <el-form-item label=\"邮箱\"><el-input v-model=\"email\"></el-input></el-form-item>\r\n            <el-form-item label=\"社保号\"><el-input v-model=\"SSN\"></el-input></el-form-item>\r\n            <el-form-item label=\"税号\"><el-input v-model=\"tax\"></el-input></el-form-item>\r\n            <el-form-item label=\"检测号\"><el-input v-model=\"detection\"></el-input></el-form-item>\r\n            <el-form-item label=\"电话号码\"><el-input v-model=\"telephone\"></el-input></el-form-item>\r\n            <el-form-item label=\"员工类型\">\r\n              <el-radio-group v-model=\"kind\">\r\n                <el-radio label=\"0\">Hourly</el-radio>\r\n                <el-radio label=\"1\">Salaried</el-radio>\r\n                <el-radio label=\"2\">Commission</el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n            <el-form-item label=\"时薪\" v-show=\"kind == 0\"><el-input v-model=\"hoursal\"></el-input></el-form-item>\r\n            <el-form-item label=\"月薪\" v-show=\"kind != 0\"><el-input v-model=\"sal\"></el-input></el-form-item>\r\n            <el-form-item label=\"佣金\" v-show=\"kind == 2\"><el-input v-model=\"com\"></el-input></el-form-item>\r\n            <el-form-item label=\"小时限制\"><el-input v-model=\"hourlimit\"></el-input></el-form-item>\r\n            <el-form-item label=\"支付方式\">\r\n              <el-radio-group v-model=\"way\">\r\n                <el-radio label=\"0\">Mail</el-radio>\r\n                <el-radio label=\"1\">Pickup</el-radio>\r\n                <el-radio label=\"2\">Deposit</el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n            <el-form-item label=\"地址\" v-show=\"way == 1\"><el-input v-model=\"address\"></el-input></el-form-item>\r\n            <el-form-item label=\"银行名称\" v-show=\"way == 2\"><el-input v-model=\"bankname\"></el-input></el-form-item>\r\n            <el-form-item label=\"银行账号\" v-show=\"way == 2\"><el-input v-model=\"banknumber\"></el-input></el-form-item>\r\n          </el-form>\r\n          <!-- 按钮 -->\r\n          <div slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"closeDialog\">取消</el-button>\r\n            <el-button type=\"primary\" @click=\"saveData\">保存</el-button>\r\n          </div>\r\n        </el-dialog>\r\n\r\n      </el-main>\r\n    </el-container>\r\n  </el-container>\r\n</template>\r\n<style>\r\n  .el-header {\r\n    background-color: #B3C0D1;\r\n    color: #333;\r\n    line-height: 60px;\r\n  }\r\n\r\n  .el-aside {\r\n    color: #333;\r\n  }\r\n</style>\r\n\r\n<script>\r\nimport { adminAdd, adminList } from '@/api/admin'\r\nexport default {\r\n  data () {\r\n    return {\r\n      activeMenu: 'show',\r\n      dialogVisible: false, // 对话框显示状态\r\n      // 表单数据\r\n      username: '',\r\n      password: '',\r\n      name: '',\r\n      email: '',\r\n      SSN: '',\r\n      tax: '',\r\n      detection: '',\r\n      telephone: '',\r\n      kind: '',\r\n      hoursal: '',\r\n      sal: '',\r\n      com: '',\r\n      hourlimit: '',\r\n      way: '',\r\n      address: '',\r\n      bankname: '',\r\n      banknumber: ''\r\n\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    handleMenuSelect (index) {\r\n      this.activeMenu = index\r\n      this.$router.push({ path: '/' + index })\r\n    },\r\n    logout () {\r\n      this.$store.dispatch('logout')\r\n      this.$router.push('/adminLogin')\r\n    },\r\n    // 以下是对话框相关的方法\r\n\r\n    // 点击添加按钮，显示对话框\r\n    showDialog () {\r\n      this.dialogVisible = true\r\n    },\r\n    // 关闭对话框\r\n    closeDialog () {\r\n      this.dialogVisible = false\r\n      this.resetForm()\r\n    },\r\n    // 重置表单数据\r\n    resetForm () {\r\n      this.$refs.form.resetFields()\r\n\r\n      // 重置表单数据\r\n      this.username = ''\r\n      this.password = ''\r\n      this.name = ''\r\n      this.email = ''\r\n      this.SSN = ''\r\n      this.tax = ''\r\n      this.detection = ''\r\n      this.telephone = ''\r\n      this.kind = ''\r\n      this.hoursal = ''\r\n      this.sal = ''\r\n      this.com = ''\r\n      this.hourlimit = ''\r\n      this.way = ''\r\n      this.address = ''\r\n      this.bankname = ''\r\n      this.banknumber = ''\r\n    },\r\n    // 保存数据\r\n    async saveData () {\r\n      // 在这里可以进行数据保存操作\r\n      const res = await adminAdd(this.username, this.password, this.name, this.email, this.SSN, this.tax, this.detection, this.telephone, this.kind, this.hoursal, this.sal, this.com, this.hourlimit, this.way, this.address, this.bankname, this.banknumber)\r\n      console.log(res)\r\n      if (res.code !== 1) {\r\n        this.$message(res.msg)\r\n        return\r\n      }\r\n      this.$message('添加成功')\r\n      this.closeDialog()\r\n    },\r\n    // 以下是分页器相关的方法\r\n    async fetchData () {\r\n      // 模拟异步获取数据\r\n      // 这里假设从后端获取了100条数据\r\n      const res = await adminList(this.currentPage, this.pageSize)\r\n      console.log(res.data.records)\r\n      this.items = res.data.records\r\n      this.totalItems = res.data.total\r\n    },\r\n    handleCurrentChange (newPage) {\r\n      // 处理页码变化\r\n      this.currentPage = newPage\r\n      this.fetchData()\r\n    },\r\n    handleSizeChange (newSize) {\r\n      // 处理页面大小变化\r\n      this.pageSize = newSize\r\n      this.currentPage = 1\r\n      this.fetchData()\r\n    }\r\n\r\n  }\r\n\r\n}\r\n</script>\r\n"],"mappings":";AA6FA,SAAAA,QAAA,EAAAC,SAAA;AACA;EACAC,KAAA;IACA;MACAC,UAAA;MACAC,aAAA;MAAA;MACA;MACAC,QAAA;MACAC,QAAA;MACAC,IAAA;MACAC,KAAA;MACAC,GAAA;MACAC,GAAA;MACAC,SAAA;MACAC,SAAA;MACAC,IAAA;MACAC,OAAA;MACAC,GAAA;MACAC,GAAA;MACAC,SAAA;MACAC,GAAA;MACAC,OAAA;MACAC,QAAA;MACAC,UAAA;IAEA;EACA;EAEAC,OAAA;IACAC,iBAAAC,KAAA;MACA,KAAArB,UAAA,GAAAqB,KAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QAAAC,IAAA,QAAAH;MAAA;IACA;IACAI,OAAA;MACA,KAAAC,MAAA,CAAAC,QAAA;MACA,KAAAL,OAAA,CAAAC,IAAA;IACA;IACA;;IAEA;IACAK,WAAA;MACA,KAAA3B,aAAA;IACA;IACA;IACA4B,YAAA;MACA,KAAA5B,aAAA;MACA,KAAA6B,SAAA;IACA;IACA;IACAA,UAAA;MACA,KAAAC,KAAA,CAAAC,IAAA,CAAAC,WAAA;;MAEA;MACA,KAAA/B,QAAA;MACA,KAAAC,QAAA;MACA,KAAAC,IAAA;MACA,KAAAC,KAAA;MACA,KAAAC,GAAA;MACA,KAAAC,GAAA;MACA,KAAAC,SAAA;MACA,KAAAC,SAAA;MACA,KAAAC,IAAA;MACA,KAAAC,OAAA;MACA,KAAAC,GAAA;MACA,KAAAC,GAAA;MACA,KAAAC,SAAA;MACA,KAAAC,GAAA;MACA,KAAAC,OAAA;MACA,KAAAC,QAAA;MACA,KAAAC,UAAA;IACA;IACA;IACA,MAAAgB,SAAA;MACA;MACA,MAAAC,GAAA,SAAAtC,QAAA,MAAAK,QAAA,OAAAC,QAAA,OAAAC,IAAA,OAAAC,KAAA,OAAAC,GAAA,OAAAC,GAAA,OAAAC,SAAA,OAAAC,SAAA,OAAAC,IAAA,OAAAC,OAAA,OAAAC,GAAA,OAAAC,GAAA,OAAAC,SAAA,OAAAC,GAAA,OAAAC,OAAA,OAAAC,QAAA,OAAAC,UAAA;MACAkB,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA,IAAAA,GAAA,CAAAG,IAAA;QACA,KAAAC,QAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MACA;MACA,KAAAD,QAAA;MACA,KAAAV,WAAA;IACA;IACA;IACA,MAAAY,UAAA;MACA;MACA;MACA,MAAAN,GAAA,SAAArC,SAAA,MAAA4C,WAAA,OAAAC,QAAA;MACAP,OAAA,CAAAC,GAAA,CAAAF,GAAA,CAAApC,IAAA,CAAA6C,OAAA;MACA,KAAAC,KAAA,GAAAV,GAAA,CAAApC,IAAA,CAAA6C,OAAA;MACA,KAAAE,UAAA,GAAAX,GAAA,CAAApC,IAAA,CAAAgD,KAAA;IACA;IACAC,oBAAAC,OAAA;MACA;MACA,KAAAP,WAAA,GAAAO,OAAA;MACA,K