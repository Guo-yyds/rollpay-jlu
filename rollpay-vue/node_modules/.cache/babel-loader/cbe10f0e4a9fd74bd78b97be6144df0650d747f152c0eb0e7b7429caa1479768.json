{"ast":null,"code":"import { adminLogin } from '@/api/login';\n// import { Toast } from 'vant'\n\nexport default {\n  name: 'LoginPage',\n  data() {\n    return {\n      mobile: '' // 手机号\n    };\n  },\n\n  async created() {},\n  methods: {\n    // 校验 手机号 和 图形验证码 是否合法\n    // 通过校验，返回true\n    // 不通过校验，返回false\n    validFn() {\n      if (!/^1[3-9]\\d{9}$/.test(this.mobile)) {\n        this.$toast('请输入正确的手机号');\n        return false;\n      }\n      return true;\n    },\n    // 登录\n    async login() {\n      if (!this.validFn()) {\n        return;\n      }\n      if (!/^\\d{6}$/.test(this.msgCode)) {\n        this.$toast('请输入正确的手机验证码');\n        return;\n      }\n      console.log('发送登录请求');\n      const res = await adminLogin(this.mobile, this.msgCode);\n      this.$store.commit('user/setUserInfo', res.data);\n      this.$toast('登录成功');\n\n      // 进行判断，看地址栏有无回跳地址\n      // 1. 如果有   => 说明是其他页面，拦截到登录来的，需要回跳\n      // 2. 如果没有 => 正常去首页\n      const url = this.$route.query.backUrl || '/';\n      this.$router.replace(url);\n    }\n  },\n  // 离开页面清除定时器\n  destroyed() {}\n};","map":{"version":3,"names":["adminLogin","name","data","mobile","created","methods","validFn","test","$toast","login","msgCode","console","log","res","$store","commit","url","$route","query","backUrl","$router","replace","destroyed"],"sources":["src/views/adminLogin.vue"],"sourcesContent":["<template>\r\n  <div class=\"login\">\r\n    <van-nav-bar title=\"管理员登录\" left-arrow @click-left=\"$router.go(-1)\" />\r\n\r\n    <div class=\"container\">\r\n      <div class=\"title\">\r\n        <h3>管理员登录</h3>\r\n      </div>\r\n\r\n      <div class=\"form\">\r\n        <div class=\"form-item\">\r\n          <input v-model=\"mobile\" class=\"inp\" maxlength=\"11\" placeholder=\"请输入手机号码\" type=\"text\">\r\n        </div>\r\n        <div class=\"form-item\">\r\n          <input v-model=\"msgCode\" class=\"inp\" placeholder=\"请输入短信验证码\" type=\"text\">\r\n          <button @click=\"getCode\">\r\n            {{ second === totalSecond ? '获取验证码' : second + '秒后重新发送'}}\r\n          </button>\r\n        </div>\r\n      </div>\r\n\r\n      <div @click=\"login\" class=\"login-btn\">登录</div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { adminLogin } from '@/api/login'\r\n// import { Toast } from 'vant'\r\n\r\nexport default {\r\n  name: 'LoginPage',\r\n  data () {\r\n    return {\r\n      mobile: '' // 手机号\r\n    }\r\n  },\r\n  async created () {\r\n  },\r\n  methods: {\r\n    // 校验 手机号 和 图形验证码 是否合法\r\n    // 通过校验，返回true\r\n    // 不通过校验，返回false\r\n    validFn () {\r\n      if (!/^1[3-9]\\d{9}$/.test(this.mobile)) {\r\n        this.$toast('请输入正确的手机号')\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n\r\n    // 登录\r\n    async login () {\r\n      if (!this.validFn()) {\r\n        return\r\n      }\r\n\r\n      if (!/^\\d{6}$/.test(this.msgCode)) {\r\n        this.$toast('请输入正确的手机验证码')\r\n        return\r\n      }\r\n\r\n      console.log('发送登录请求')\r\n\r\n      const res = await adminLogin(this.mobile, this.msgCode)\r\n      this.$store.commit('user/setUserInfo', res.data)\r\n      this.$toast('登录成功')\r\n\r\n      // 进行判断，看地址栏有无回跳地址\r\n      // 1. 如果有   => 说明是其他页面，拦截到登录来的，需要回跳\r\n      // 2. 如果没有 => 正常去首页\r\n      const url = this.$route.query.backUrl || '/'\r\n      this.$router.replace(url)\r\n    }\r\n  },\r\n  // 离开页面清除定时器\r\n  destroyed () {\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.container {\r\n  padding: 49px 29px;\r\n\r\n  .title {\r\n    margin-bottom: 20px;\r\n    h3 {\r\n      font-size: 26px;\r\n      font-weight: normal;\r\n    }\r\n    p {\r\n      line-height: 40px;\r\n      font-size: 14px;\r\n      color: #b8b8b8;\r\n    }\r\n  }\r\n\r\n  .form-item {\r\n    border-bottom: 1px solid #f3f1f2;\r\n    padding: 8px;\r\n    margin-bottom: 14px;\r\n    display: flex;\r\n    align-items: center;\r\n    .inp {\r\n      display: block;\r\n      border: none;\r\n      outline: none;\r\n      height: 32px;\r\n      font-size: 14px;\r\n      flex: 1;\r\n    }\r\n    img {\r\n      width: 94px;\r\n      height: 31px;\r\n    }\r\n    button {\r\n      height: 31px;\r\n      border: none;\r\n      font-size: 13px;\r\n      color: #cea26a;\r\n      background-color: transparent;\r\n      padding-right: 9px;\r\n    }\r\n  }\r\n\r\n  .login-btn {\r\n    width: 100%;\r\n    height: 42px;\r\n    margin-top: 39px;\r\n    background: linear-gradient(90deg,#ecb53c,#ff9211);\r\n    color: #fff;\r\n    border-radius: 39px;\r\n    box-shadow: 0 10px 20px 0 rgba(0,0,0,.1);\r\n    letter-spacing: 2px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AA2BA,SAAAA,UAAA;AACA;;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;IACA;EACA;;EACA,MAAAC,QAAA,GACA;EACAC,OAAA;IACA;IACA;IACA;IACAC,QAAA;MACA,qBAAAC,IAAA,MAAAJ,MAAA;QACA,KAAAK,MAAA;QACA;MACA;MACA;IACA;IAEA;IACA,MAAAC,MAAA;MACA,UAAAH,OAAA;QACA;MACA;MAEA,eAAAC,IAAA,MAAAG,OAAA;QACA,KAAAF,MAAA;QACA;MACA;MAEAG,OAAA,CAAAC,GAAA;MAEA,MAAAC,GAAA,SAAAb,UAAA,MAAAG,MAAA,OAAAO,OAAA;MACA,KAAAI,MAAA,CAAAC,MAAA,qBAAAF,GAAA,CAAAX,IAAA;MACA,KAAAM,MAAA;;MAEA;MACA;MACA;MACA,MAAAQ,GAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,OAAA;MACA,KAAAC,OAAA,CAAAC,OAAA,CAAAL,GAAA;IACA;EACA;EACA;EACAM,UAAA,GAEA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}